pipeline {
    agent any
    
    evironmet{
        PATH = "/opt/maven/bin:$PATH"
    }

    stages {
        stage('get code') {
            steps {
               git 'https://github.com/ekanadha/sparkjava-war-example.git'
            }
        }
        stage('Build') {
            steps {
                sh "mvn clean package"
            }
        }
    }
}




